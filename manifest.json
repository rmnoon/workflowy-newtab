{
  "manifest_version": 2,
  "name": "Workflowy New Tab",
  "version": "1.0.3",
  "short_name": "WorkflowyTab",
  
  "description": "Make a Workflowy list your Chrome New Tab page.",
  "icons": {
    "24": "icon_24.png",
    "32": "icon_32.png",
    "64": "icon_64.png",
    "128": "icon_128.png",
    "512": "icon_512.png"
  },
  "homepage_url": "https://github.com/rmnoon/workflowy-newtab",  

  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_page": "options.html"
}